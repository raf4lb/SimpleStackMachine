; sum(x <i16>, y <i16>) {
;   return x + y
; } <i16>
; 
; main() {
;   result <i16>
;   result = sum(1, -2)
;   print(result)
; } <void>
; expected output: -1


fn .main
    ALLOC_LOCAL 2 $result
    PUSHL_I16 1
    PUSHL_I16 -2
    CALL .sum
    POP_LOCAL 2 $result
    PUSH_LOCAL 2 $result
    TOP_I16
    HALT

fn .sum
    ALLOC_LOCAL 2 $y
    POP_LOCAL 2 $y
    ALLOC_LOCAL 2 $x
    POP_LOCAL 2 $x
    PUSH_LOCAL 2 $x
    PUSH_LOCAL 2 $y
    ADD_I16
    RETURN