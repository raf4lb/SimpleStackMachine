    PUSHL_U16 2
    PUSHL_U16 4
    PUSH_LOCAL
    
    PUSHL_U16 2
    PUSHL_U16 2
    PUSH_LOCAL

    PUSHL_U16 2
    PUSHL_U16 0
    PUSH_LOCAL

    PUSHL_U16 $print
    SYSCALL 0

    RETURN
