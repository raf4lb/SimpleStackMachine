    PUSHL_U16 2
    ADD_U16
    PUSHL_U16 2  ; size
    PUSHL_U16 $index
    POP_LOCAL

    PUSHL_U16 3  ; value
    PUSHL_U16 2  ; size
    PUSHL_U16 2
    PUSHL_U16 $index  ; address
    PUSH_LOCAL
    POP_LOCAL
    
    PUSHL_U16 2
    PUSHL_U16 $index
    PUSH_LOCAL
    PUSHL_U16 2
    ADD_U16
    PUSHL_U16 2  ; size
    PUSHL_U16 $index
    POP_LOCAL

    PUSHL_U16 5  ; value
