fn .main
    PUSHL_U16 1
    CALL .incr
    TOP_U16
    HALT

fn .incr
    ALLOC_LOCAL 2 $i
    POP_LOCAL 2 $i
    PUSH_LOCAL 2 $i
    PUSHL_U16 10
    COMPARE_EQ
    POP_JUMP_IF_FALSE .call_incr
    PUSH_LOCAL 2 $i
    RETURN
    .call_incr
        PUSH_LOCAL 2 $i
        PUSHL_U16 1
        ADD_U16
        CALL .incr
        RETURN

